{
  "compilerOptions": {
    "target": "es2016",
    "module": "ESNext",
	  "moduleResolution": "Node",
    "outDir": "./dist",
    "esModuleInterop": true,
	  "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "declaration": true,
    "noImplicitOverride": true
  }
}
