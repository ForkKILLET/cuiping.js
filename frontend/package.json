{
  "name": "cuiping-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "VITE_BUILD_TIME=\"$(date -R)\" VITE_BUILD_ENV=\"$(uname -sr)\" VITE_LAST_COMMIT=\"$(git log -1 HEAD --oneline --no-color --no-decorate)\" vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.cypress-ct.json --composite false"
  },
  "dependencies": {
    "cuiping": "workspace:^1.10.0",
    "cuiping-component": "workspace:^1.5.0",
    "cuiping-monaco": "workspace:^1.0.0",
    "entities": "^4.4.0",
    "monaco-editor": "^0.34.1",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vue": "^3.2.45",
    "vue-i18n": "^9.2.2"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/tsconfig": "^0.1.3",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.9.4",
    "vite": "3.2.0",
    "vue-tsc": "^1.0.22"
  },
  "license": "MIT"
}
